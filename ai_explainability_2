  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "94fb4410",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-05-24T20:50:29.912890Z",
     "iopub.status.busy": "2024-05-24T20:50:29.912506Z",
     "iopub.status.idle": "2024-05-24T20:50:31.126382Z",
     "shell.execute_reply": "2024-05-24T20:50:31.125227Z"
    },
    "papermill": {
     "duration": 1.410026,
     "end_time": "2024-05-24T20:50:31.129141",
     "exception": false,
     "start_time": "2024-05-24T20:50:29.719115",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x500 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import tensorflow as tf\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "num_samples = 50\n",
    "noise_level = 0.1#noise level\n",
    "\n",
    "def add_noise(image, noise_level):\n",
    "    noise = tf.random.normal(shape=image.shape, mean=0.0, stddev=noise_level)\n",
    "    noisy_image = tf.clip_by_value(image + noise, 0.0, 1.0)\n",
    "    return noisy_image\n",
    "\n",
    "perturbed_gradients = []\n",
    "for _ in range(num_samples):\n",
    "    perturbed_img = add_noise(img_tensor, noise_level)\n",
    "    with tf.GradientTape() as tape:\n",
    "        tape.watch(perturbed_img)\n",
    "        prediction = model(perturbed_img)\n",
    "        loss = prediction[0, tf.argmax(prediction[0])]\n",
    "    gradient = tape.gradient(loss, perturbed_img)\n",
    "    perturbed_gradients.append(gradient)\n",
    "smoothed_gradient = tf.reduce_mean(perturbed_gradients, axis=0)\n",
    "smoothed_gradient = tf.abs(smoothed_gradient)\n",
    "smoothed_gradient = smoothed_gradient[0]\n",
    "smoothed_gradient = (smoothed_gradient - tf.reduce_min(smoothed_gradient)) / (tf.reduce_max(smoothed_gradient) - tf.reduce_min(smoothed_gradient))\n",
    "plt.figure(figsize=(10, 5))\n",
    "plt.subplot(1, 2, 1)\n",
    "plt.title(f\"Original Image (Label: {y_test[img_index]})\")\n",
    "plt.imshow(x_train[img_index], cmap='gray')\n",
    "plt.subplot(1, 2, 2)\n",
    "plt.title(\"SmoothGrad Saliency Map\")\n",
    "plt.imshow(smoothed_gradient.numpy())\n",
    "plt.show()\n"
   ]
  },
